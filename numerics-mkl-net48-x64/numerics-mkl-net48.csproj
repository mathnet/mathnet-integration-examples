<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net48</TargetFramework>
		<RuntimeIdentifiers>win-x64;linux-x64</RuntimeIdentifiers>
		<PlatformTarget>x64</PlatformTarget>
		<RootNamespace>numerics_mkl_net48</RootNamespace>
		<AssemblyName>numerics-mkl-net48</AssemblyName>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="MathNet.Numerics" Version="5.0.0-alpha10" />
		<PackageReference Include="MathNet.Numerics.MKL.Linux-x64" Version="3.0.0-beta3" />
		<PackageReference Include="MathNet.Numerics.MKL.Win-x64" Version="3.0.0-beta3" />
		<PackageReference Include="MathNet.Numerics.Providers.MKL" Version="5.0.0-alpha10" />
		<PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies" Version="1.0.2">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>

</Project>
